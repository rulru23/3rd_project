<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ldg.subway.side.cookie.CookieDAO">
	<select id="getAllCookie" resultType="CookieVO">
		Select *
		from Cookie
	</select>
	
	<select id="getAllCookieById" resultType="CookieVO">
		Select *
		from Cookie
		where cookie_id = #{cookie_id}
	</select>
</mapper>