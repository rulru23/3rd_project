<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ldg.subway.sandwich.cheese.CheeseDAO">
	<select id="getAllCheese" resultType="CheeseVO">
		Select *
		from cheese
	</select>
		<select id="getCheeseById" resultType="CheeseVO">
		Select *
		from cheese
		where cheese_id=#{cheese_id}
	</select>
</mapper>