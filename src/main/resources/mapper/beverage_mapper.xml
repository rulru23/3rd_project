<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ldg.subway.side.beverage.BeverageDAO">
	<select id="getAllBeverage" resultType="BeverageVO">
		Select *
		from Beverage
	</select>
	
		<select id="getBeverageById" resultType="BeverageVO">
		Select *
		from Beverage
		where beverage_id = #{beverage_id}
	</select>
</mapper>